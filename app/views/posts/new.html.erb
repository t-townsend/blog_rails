<h1>New Posts!</h1>

<%= form_for @posts do |f| %>
<div>
<%= f.label :user %>
<%= f.text_field :user %>
</div>
<div>
<%= f.label :image %>
<%= f.text_field :image %>
</div>
<div>
<%= f.label :title %>
<%= f.text_field :title %>
</div>
<div>
<%= f.label :body %>
<%= f.text_field :body %>
</div>
<%= f.submit %>
<%end %>
